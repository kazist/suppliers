    "query": {
        "from": {
            "table": "Suppliers\\Suppliers\\Code\\Tables\\Suppliers",
            "alias": "ss",
            "name": "#__suppliers_suppliers"
        },
        "select": [
            "ss.id",
            "ss.title",
            "ss.category_id",
            "sc.title AS category_id_title",
            "ss.description",
            "ss.logo",
            "mm.title AS logo_title",
            "mm.file AS logo_file",
            "ss.email",
            "ss.phone",
            "ss.address",
            "ss.website",
            "ss.published",
            "ss.created_by",
            "ss.date_created",
            "ss.modified_by",
            "ss.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Suppliers\\Categories\\Code\\Tables\\Categories",
                "alias": "sc",
                "name": "#__suppliers_categories",
                "on": "ss.category_id = sc.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "ss.logo = mm.id"
            }
        ]
    }
